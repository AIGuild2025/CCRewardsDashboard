# Ignore environment files with API keys
.env
.env.local

# Ignore encryption key (CRITICAL - contains master key)
.encryption_key
*.key

# Optionally commit secrets.yaml.enc (encrypted, relatively safe)
# Uncomment the line below to prevent committing encrypted passwords:
# secrets.yaml.enc

# Ignore Python cache
__pycache__/
*.pyc
*.pyo
*.pyd
.Python

# Ignore virtual environment
venv/
env/
ENV/

# Ignore IDE files
.vscode/
.idea/
*.swp
*.swo

# Ignore output files
output/*.csv
output/*.json
!output/README.md

# Ignore sample PDFs (privacy)
sample_pdfs/*.pdf
!sample_pdfs/README.md

# Ignore test artifacts
.pytest_cache/
.coverage
htmlcov/

# Ignore OS files
.DS_Store
Thumbs.db
